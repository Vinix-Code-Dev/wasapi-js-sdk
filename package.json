{
  "name": "@wasapi/js-sdk",
  "version": "0.1.33",
  "description": "SDK oficial en JavaScript (Typescript) para Wasapi.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./models": {
      "types": "./dist/types/wasapi/models/index.d.ts",
      "import": "./dist/esm/wasapi/models/index.js",
      "require": "./dist/cjs/wasapi/models/index.js"
    }
  },
  "scripts": {
    "lint": "eslint . --no-ignore",
    "dev": "npm run lint && nodemon ./src/app.ts",
    "build": "rollup -c rollup.config.mjs",
    "prepare": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "keywords": [
    "wasapi",
    "whatsapp",
    "sdk"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanalvarezPro/wasapi-sdk"
  },
  "dependencies": {
    "axios": "^1.11.0"
  },
  "devDependencies": {
    "@types/mime-types": "^3.0.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "@types/express": "^5.0.3",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.52.0",
    "nodemon": "^3.1.0",
    "rollup": "^4.10.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "author": {
    "name": "Juan Camilo Alvarez",
    "email": "camilo@wasapi.io"
  },
  "license": "ISC"
}
