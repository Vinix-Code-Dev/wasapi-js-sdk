{
  "name": "wasapi-sdk",
  "version": "0.1.3",
  "description": "SDK oficial para Wasapi (WhatsApp, campaÃ±as, contactos, flujos, bots etc.)",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./provider": {
      "types": "./dist/types/wasapi/provider/index.d.ts",
      "import": "./dist/esm/wasapi/provider/index.js",
      "require": "./dist/cjs/wasapi/provider/index.js"
    },
    "./models": {
      "types": "./dist/types/wasapi/models/index.d.ts",
      "import": "./dist/esm/wasapi/models/index.js",
      "require": "./dist/cjs/wasapi/models/index.js"
    }
  },
  "scripts": {
    "lint": "eslint . --no-ignore",
    "dev": "npm run lint && nodemon ./src/app.ts",
    "build": "rollup -c",
    "prepare": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "keywords": [
    "wasapi",
    "whatsapp",
    "sdk"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanalvarezPro/wasapi-sdk"
  },
  "dependencies": {
    "axios": "^1.11.0"
  },
  "peerDependencies": {
    "@builderbot/bot": ">=1.2.9"
  },
  "peerDependenciesMeta": {
    "@builderbot/bot": { "optional": true }
  },
  "devDependencies": {
    "@types/mime-types": "^3.0.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "@types/express": "^5.0.3",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.52.0",
    "eslint-plugin-builderbot": "latest",
    "nodemon": "^3.1.0",
    "rollup": "^4.10.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "author": "juanalvarez.pro",
  "license": "ISC"
}
